    "@tiptap/pm": "^2.7.0",
    "@vueuse/components": "^13.1.0",
    "@vueuse/core": "^10.3.0",
    "date-fns": "^4.1.0",
    "dropzone": "^6.0.0-beta.2",
    "frappe-ui": "0.1.175",
    "html2pdf.js": "^0.10.3",
    "idb-keyval": "^6.2.1",
    "mammoth": "^1.6.0",
    "mitt": "^3.0.0",
    "sass": "^1.62.0",
    "slugify": "^1.6.6",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.0",
