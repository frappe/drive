    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.4.17",
