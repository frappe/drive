{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "debug": "vite build --mode=development --base=/assets/drive/frontend/ && yarn copy-html-entry",
    "build": "vite build --base=/assets/drive/frontend/ && yarn copy-html-entry",
    "copy-html-entry": "cp ../drive/public/frontend/index.html ../drive/www/drive.html",
    "preview": "vite preview",
    "precommit": "pretty-quick --staged",
    "prepare": "cd .. && husky install frontend/.husky",
    "lint": "eslint --fix --ext .js,.vue ."
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.8",
    "@tiptap/core": "^2.0.3",
    "@tiptap/extension-underline": "^2.0.3",
    "@tiptap/pm": "^2.0.0-beta.220",
    "@tiptap/starter-kit": "^2.0.0-beta.220",
    "@tiptap/vue-3": "^2.0.0-beta.220",
    "canvas-datagrid": "^0.4.5",
    "docx-preview": "^0.1.15",
    "dropzone": "^6.0.0-beta.2",
    "feather-icons": "^4.29.0",
    "frappe-ui": "0.0.101",
    "fuzzysort": "^2.0.4",
    "lucide-vue-next": "^0.206.0",
    "mitt": "^3.0.0",
    "sass": "^1.62.0",
    "vue": "^3.2",
    "vue-inline-svg": "^2.1.3",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0"
  }
